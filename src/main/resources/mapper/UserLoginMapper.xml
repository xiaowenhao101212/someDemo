<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xquant.example.appservice.mapper.UserLoginMapper">

    <select id="selectByUserCode" resultType="com.xquant.example.appservice.domain.entity.UserLogin">
        select * from t_user where a_user_code = #{aUserCode} limit 1
    </select>

</mapper>